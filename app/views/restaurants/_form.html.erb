<%= form_for @restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :street %>
  <%= f.text_field :street %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :postcode %>
  <%= f.text_field :postcode %>

  <%= f.label :tel %>
  <%= f.text_field :tel %>

  <%= f.label :kind %>
  <%= f.text_field :kind %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit %>
<% end %>